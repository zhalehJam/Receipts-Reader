    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run Receipt Processor",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/main.py",
        "console": "integratedTerminal",
        "justMyCode": true
      },
      {
        "name": "Containers: Python - Fastapi",
        "type": "docker",
        "request": "launch",
        "preLaunchTask": "docker-run: debug",
        "python": {
          "pathMappings": [
            {
              "localRoot": "${workspaceFolder}",
              "remoteRoot": "/app"
            }
          ],
          "projectType": "fastapi"
        }
      }
    ],

